{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowModelBucketListAndLocation",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": "arn:aws:s3:::phdata-housing-models"
        },
        {
            "Sid": "AllowModelRead",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion"
            ],
            "Resource": "arn:aws:s3:::phdata-housing-models/models/housing/*"
        },
        {
            "Sid": "AllowModelWrite",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::phdata-housing-models/models/housing/*"
        }
    ]
}
